@model Snack

<div class="col-md-3" style="justify-content: space-between">
    <div class="card" style="width: 18rem;">
        <img src="@Model.ImageURL" class="card-img-top" alt="...">
        <div class="card-body">
            <h5 class="card-title">@Model.SnackName</h5>
            <p class="card-text">@Model.Price.ToString("c")</p>
            <a href="#" class="btn btn-primary">Go somewhere</a>
        </div>
    </div>
</div>